{
  "name": "activa-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "migration:run": "npx ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:run -d src/ormconfig.ts",
    "migration:generate": "npx ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:generate src/database/migrations/migration -d src/ormconfig.ts",
    "migration:revert": "npx ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:revert -d src/ormconfig.ts"
  },
  "dependencies": {
    "@nestjs/common": "11.0.0",
    "@nestjs/config": "3.3.0",
    "@nestjs/core": "11.0.0",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/passport": "11.0.3",
    "@nestjs/platform-express": "11.0.0",
    "@nestjs/schedule": "4.1.2",
    "@nestjs/swagger": "11.0.2",
    "@nestjs/typeorm": "10.0.2",
    "argon2": "0.41.1",
    "axios": "1.7.9",
    "class-transformer": "^0.5.1",
    "class-validator": "0.14.1",
    "dotenv": "16.4.7",
    "joi": "^17.13.3",
    "mysql2": "3.13.0",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "0.2.0",
    "rxjs": "7.8.1",
    "typeorm": "0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "10.0.0",
    "@nestjs/schematics": "10.0.0",
    "@nestjs/testing": "10.0.0",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.1",
    "@types/supertest": "6.0.0",
    "@typescript-eslint/eslint-plugin": "8.0.0",
    "@typescript-eslint/parser": "8.0.0",
    "eslint": "8.0.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "jest": "29.5.0",
    "prettier": "3.0.0",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.3"
  }
}
